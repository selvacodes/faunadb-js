{
  "name": "faunadb-js-steida",
  "version": "0.1.0",
  "description": "Unofficial Fauna JS driver",
  "main": "./dist/cjs/index.js",
  "module": "./dist/es6/index.js",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "repository": "https://github.com/steida/faunadb-js.git",
  "author": "Daniel Steigerwald <daniel@steigerwald.cz>",
  "license": "MIT",
  "scripts": {
    "deps": "rimraf 'yarn.lock' 'node_modules' && yarn",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint '**/*.{ts,tsx}'",
    "jest": "jest src",
    "build-lib:cjs": "tsc -p tsconfig.lib.cjs.json",
    "build-lib:es6": "tsc -p tsconfig.lib.es6.json",
    "build-lib:types": "tsc -p tsconfig.lib.types.json",
    "build-lib": "yarn clean-dist && yarn build-lib:cjs && yarn build-lib:es6 && yarn build-lib:types",
    "prepare": "yarn build-lib",
    "test": "yarn lint && yarn tsc && yarn jest && yarn build-lib",
    "docs": "docs-ts && mv docs ../../docs",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "base64-js": "^1.3.1",
    "btoa-lite": "^1.0.0",
    "cross-fetch": "^3.0.4",
    "url-parse": "^1.4.7"
  },
  "peerDependencies": {
    "fp-ts": "^2.5.3",
    "io-ts": "^2.1.3",
    "io-ts-types": "^0.5.6",
    "monocle-ts": "^2.1.0",
    "newtype-ts": "^0.3.4"
  },
  "devDependencies": {
    "@types/base64-js": "^1.2.5",
    "@types/btoa-lite": "^1.0.0",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.33",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "docs-ts": "^0.4.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "faunadb": "^2.13.1",
    "fp-ts": "^2.5.3",
    "io-ts": "^2.1.3",
    "io-ts-types": "^0.5.6",
    "jest": "^25.2.7",
    "monocle-ts": "^2.1.0",
    "newtype-ts": "^0.3.4",
    "next": "^9.3.4",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  }
}
